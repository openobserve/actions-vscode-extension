{
  "name": "openobserve",
  "displayName": "openobserve",
  "description": "Actions",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "viewsWelcome": [
      {
        "view": "debug",
        "contents": "In order to run and debug, you'll need to create a local workspace."
      },
      {
        "view": "terminal",
        "contents": "Terminals are disabled in this environment for security reasons."
      },
      {
        "view": "deployView",
        "contents": "\nDeploy your changes.\n\n[Deploy (⇧⌘U)](command:openobserve.action.deploy)\n\n"
      },
      {
        "view": "deployViewExplorer",
        "contents": "\nDeploy your changes.\n\n[Deploy (⇧⌘U)](command:openobserve.action.deploy)\n\n"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "deploySidebar",
          "title": "Deploy",
          "icon": "resources/openobserve.svg"
        }
      ]
    },
    "views": {
      "deploySidebar": [
        {
          "id": "deployView",
          "name": "Deploy Actions"
        }
      ],
      "explorer": [
        {
          "id": "deployViewExplorer",
          "name": "Deploy Actions",
          "visibility": "visible",
          "order": 2
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "jszip": "^3.10.1"
  }
}
